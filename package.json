{
	"name": "@making-sense/vtl-2-0-antlr-tools-ts",
	"version": "0.2.3",
	"description": "VTL 2.0 Typescript tools generated by ANTLR",
	"repository": {
		"type": "git",
		"url": "git://github.com/Making-Sense-Info/VTL-2.0-ANTLR-Tools-TS.git"
	},
	"main": "dist/lib/index.js",
	"types": "dist/lib/index.d.ts",
	"scripts": {
		"build-grammar": "cd src/lib && antlr4ts -no-visitor -no-listener Vtl.g4 -o ./generated",
		"g4ToTs": "ts-node src/bin/g4toTs",
		"build": "yarn build-grammar && yarn g4ToTs && tsc"
	},
	"author": "u/NicoLaval",
	"license": "MIT",
	"files": [
		"src/",
		"!src/test/",
		"dist/",
		"!dist/test/",
		"!dist/tsconfig.tsbuildinfo"
	],
	"keywords": [
		"VTL",
		"ANTLR"
	],
	"homepage": "https://github.com/Making-Sense-Info/VTL-2.0-ANTLR-Tools-TS",
	"devDependencies": {
		"@types/node": "^20.8.0",
		"antlr4ts": "^0.5.0-alpha.4",
		"antlr4ts-cli": "^0.5.0-alpha.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {},
	"publishConfig": {
		"access": "public"
	}
}
