{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "es2015",
		"lib": ["es6"],
		"esModuleInterop": true,
		"declaration": true,
		"outDir": "./dist",
		"sourceMap": true,
		"newLine": "LF",
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"incremental": true,
		"strict": true,
		"downlevelIteration": true,
		"jsx": "react-jsx",
		"noFallthroughCasesInSwitch": true,
		"strictPropertyInitialization": false
	},
	"include": ["src"]
}
