{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES2020",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"sourceMap": true,
		"declaration": true,
		"outDir": "./dist",
		"newLine": "LF",
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"incremental": true,
		"strict": true,
		"downlevelIteration": true,
		"jsx": "react-jsx",
		"noFallthroughCasesInSwitch": true,
		"strictPropertyInitialization": false
	},
	"include": ["src"]
}
